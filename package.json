{
  "name": "prismic-cli",
  "description": "Command line tool to bootstrap prismic projects.",
  "version": "3.8.3",
  "author": "prismic.io developers",
  "bin": {
    "prismic": "bin/run"
  },
  "bugs": "https://github.com/prismicio/prismic-cli/issues",
  "contributors": [
    "Marc McIntosh <marcmcintosh1987@gmail.com>"
  ],
  "dependencies": {
    "@babel/generator": "^7.12.11",
    "@babel/parser": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/adm-zip": "^0.4.33",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.10",
    "@types/qs": "^6.9.5",
    "@types/yeoman-generator": "^4.11.3",
    "adm-zip": "^0.5.1",
    "axios": "^0.21.0",
    "cli-ux": "^5.5.1",
    "cookie": "^0.4.1",
    "create-nuxt-app": "^3.5.0",
    "inquirer": "^7.3.3",
    "qs": "^6.9.4",
    "sinon": "^9.2.1",
    "tmp-promise": "^3.0.2",
    "tslib": "^1",
    "yeoman-environment": "^2.10.3",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/babel__generator": "^7.6.2",
    "@types/babel__traverse": "^7.11.0",
    "@types/chai": "^4",
    "@types/cookie": "^0.4.0",
    "@types/mocha": "^5",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "husky": "^4.3.0",
    "mocha": "^5",
    "nock": "^13.0.5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src/generators"
  ],
  "homepage": "https://github.com/prismicio/prismic-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "prismic",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": "prismicio/prismic-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
